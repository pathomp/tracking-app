{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000/",
  "dependencies": {
    "express": "^4.16.3",
    "google-maps-react": "^2.0.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "supertest": "^3.1.0"
  },
  "scripts": {
    "start": "node server.js",
    "start-dev": "npm-run-all --parallel start-dev-api start-dev-ui",
    "start-dev-api": "json-server --watch api/db.json --routes api/routes.json api/app.js --port 5000",
    "start-dev-ui": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "json-server": "^0.14.0",
    "npm-run-all": "^4.1.3"
  }
}
